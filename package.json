{
  "name": "codenames",
  "version": "0.1.0",
  "description": "Realtime webapp implementation of the classic social word game, Codenames. Edit",
  "author": "Joshua Porter <joshporter1@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "node build/dev-server.js",
    "start": "npm run dev",
    "build": "node build/build.js",
    "lint": "eslint --ext .js,.vue src",
